<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js" type="text/javascript"></script>

@model IEnumerable<CareDataModels.Models.Patient>
<div id="dlgAssignPictureAndVideo" style="background-color:#E5e5e5">
    <div class="smallgraytext_New">
        Search Patient 
        @using (Ajax.BeginForm("AssignPicturesAndVideos", "Staff", new AjaxOptions { HttpMethod = "Get", InsertionMode = InsertionMode.Replace, UpdateTargetId = "PatientList" }))
        {
            @Html.AntiForgeryToken() 
            @Html.TextBox("Patienttxt")
            <input class="textboxrowbutton" type="submit" value="Search">
            <br /><br /> <br /><br />
        }
    <div id="PatientList" style="text-decoration:none;background-color:#fff">
        <div id="peoplelinksmenu" align="center">
            @foreach (var item in Model)
            {
                <a title="Patient Info" style="text-decoration:none">
                    &nbsp;&nbsp;&nbsp;
                    @Html.DisplayFor(modelItem => item.FullName)
                    <span style="text-align:right; float:right;margin-right:30px;">@Html.DisplayFor(modelItem => item.Phone) </span>
                </a>
            }
        </div>
    </div>
        @using (Html.BeginForm("AssignPicturesAndVideos", "Staff", "FormMethod.Post"))
        {  
            <br /><br /><br /><br />
            <input class="textboxrowbutton" type="submit" value="Assign">
        }
    </div>
</div>
<script type="text/javascript">
    $(document).ready(function () {
        $(function () {
            $("#dlgAssignPictureAndVideo").dialog({
                model: true,
                autoOpen: true,
                resizable: false,
                width: 500,
                height: 350
            });
        })
    });
</script>