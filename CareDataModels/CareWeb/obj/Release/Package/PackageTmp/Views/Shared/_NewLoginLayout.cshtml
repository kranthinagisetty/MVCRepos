<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <link rel="stylesheet" href="@Url.Content("~/Styles/Style.css")" type="text/css" />
    <link rel="stylesheet" href="@Url.Content("~/Content/jquery-ui.css")" type="text/css" />

    <script src="@Url.Content("~/Scripts/jquery-2.1.1.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.11.1.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-timepicker.js")" type="text/javascript"></script>

</head>
<body>
    <div class="content-body">
        <table style="padding: 0px; border: 0; border-collapse: collapse; border-spacing: 0;">
            <tr>
                <td style="vertical-align: top">
                    <table style="padding: 0px; border: 0; border-collapse: collapse; border-spacing: 0;  ">
                        <tr style="height: 188px;background-color:#e6f5f7">
                            <td rowspan="2" style="vertical-align: top;padding: 0px; margin-top: -2px;">
                                <div class="left-col">
                               
                                </div>
                                @*<div id="lft-pane">
                                    @RenderSection("leftMenu", false)
                                </div>*@
                            </td>
                            @*<td style="vertical-align: central">
                               <div>
                                    <div class="pageHeader" style="width:200px">
                                        @ViewBag.Title
                                    </div>
                                </div>
                            </td>*@
                        </tr>
                        <tr>
                            <td style="vertical-align: top">
                                <div class="content-pane">
                                    @RenderBody()
                                    @RenderSection("Scripts", false)
                                </div>
                                <br />
                                <div class="footerStyle" style="border-top: 1px solid #CCC;">
                                    <label style="float: right; padding-right: 5px; font-size: 11px;color:#4e4e4e;">
                                        &copy; Copyrights 2014 &nbsp;|&nbsp;
                                        Developed by Ascendia
                                    </label>
                                </div>
                            </td>
                        </tr>
                    </table>
                </td>
            </tr>
        </table>


    </div>
</body>
</html>


